local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
    Name = "Jailbreak Mod", --Title
    LoadingTitle = "Official Jailbreak Mod",
    LoadingSubtitle = "by RC",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil, -- Create a custom folder for your hub/game
        FileName = "JbMod"
    },
        Discord = {
            Enabled = false,
            Invite = "sirius", -- The Discord invite code, do not include discord.gg/
            RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
        Title = "Jailbreak Mod",
        Subtitle = "Key System",
        Note = "Join the discord (Soon)",
        FileName = "RCKey",
        SaveKey = true,
        GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
        Key = "JbModOfficialKey" --Change the key to anything you want
    }
})
local Tab = Window:CreateTab("Vehicles", 8733342027) -- Title, Image
local Section = Tab:CreateSection("GUI")

local Button = Tab:CreateButton({
    Name = "GaragePos",
    Callback = function()
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Position = UDim2.new(0.525, 0,0.804, 0) 
    end,
})

local Button = Tab:CreateButton({
    Name = "GarageSize",
    Callback = function()
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Size = UDim2.new(0.486, 0,0.334, 0)
    end,
})

local Button = Tab:CreateButton({
    Name = "GarageAll",
    Callback = function()
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Size = UDim2.new(0.486, 0,0.334, 0)
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Position = UDim2.new(0.525, 0,0.804, 0) 
    end,
})

local Button = Tab:CreateButton({
    Name = "Do Everything",
    Callback = function()
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Size = UDim2.new(0.486, 0,0.334, 0)
        game.Players.LocalPlayer.PlayerGui.AppUI.Garage.Position = UDim2.new(0.525, 0,0.804, 0)
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer:Destroy()
    end,
})

local Button = Tab:CreateButton({
    Name = "Better Speedometer",
    Callback = function()
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.Missiles.Visible = false
				game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.MissileBuy.Visible = false
				game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.Lock.Visible = false
				game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.Plate.Visible = false
				game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.Eject.Visible = false
				game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Bottom.Meter.Odometer.Visible = false
    end,
})

local Toggle = Tab:CreateToggle({
    Name = "Speedometer Hide",
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(h)
     if h then
         game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Visible = false
     else
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Visible = true
     end
    end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "Hide UI",
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(h)
     if h then
         game.Players.LocalPlayer.PlayerGui.AppUI.Enabled = false
         game.Players.LocalPlayer.PlayerGui.LevelGui2.Enabled = false
         game.Players.LocalPlayer.PlayerGui.HotbarGui.Enabled = false
     else
        game.Players.LocalPlayer.PlayerGui.AppUI.Enabled = true
        game.Players.LocalPlayer.PlayerGui.LevelGui2.Enabled = true
        game.Players.LocalPlayer.PlayerGui.HotbarGui.Enabled = true
     end
    end,
 })

local Keybind = Tab:CreateKeybind({
   Name = "Test",
   CurrentKeybind = "Y",
   HoldToInteract = false,
   Flag = "Keybind1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(bigdog)
    if bigdog then
        game.Players.LocalPlayer.PlayerGui.AppUI.Enabled = false
         game.Players.LocalPlayer.PlayerGui.LevelGui2.Enabled = false
         game.Players.LocalPlayer.PlayerGui.HotbarGui.Enabled = false
     else
        game.Players.LocalPlayer.PlayerGui.AppUI.Enabled = true
        game.Players.LocalPlayer.PlayerGui.LevelGui2.Enabled = true
        game.Players.LocalPlayer.PlayerGui.HotbarGui.Enabled = true
     end
   end,
})

local Slider = Tab:CreateSlider({
    Name = "FoV",
    Range = {1, 120},
    Increment = 1,
    Suffix = "Your Fov",
    CurrentValue = 70,
    Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(s)
       workspace.Camera.FieldOfView = s
    end,
 })

 local Button = Tab:CreateButton({
    Name = "Reset FoV",
    Callback = function()
        workspace.Camera.FieldOfView = 70 
    end,
})

local ColorPicker = Tab:CreateColorPicker({
    Name = "Speedometer Color",
    Color = Color3.fromRGB(255,255,255),
    Flag = "ColorPicker1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(q)
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.UIStroke.Color = q
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Bottom.Line0.BackgroundColor3 = q
        game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Bottom.Line1.BackgroundColor3 = q
    end
})


local Tab = Window:CreateTab("Misc", 8733343021) -- Title, Image
local Section = Tab:CreateSection("Misc")

local Button = Tab:CreateButton({
    Name = "Kill GUI",
    Callback = function()
        Rayfield:Destroy()
    end,
})

local Button = Tab:CreateButton({
    Name = "Save game",
    Callback = function()
        saveinstance()
    end,
})

local Section = Tab:CreateSection("Credit")

local Label = Tab:CreateLabel("Owner: RC")
local Label = Tab:CreateLabel("Speedtest: RV")
local Label = Tab:CreateLabel("LIB: Rayfield")
local Label = Tab:CreateLabel("Discord: https://discord.gg/TmRmJZbE")


local enabled = false
local VehicleCFrame
local Vehicle
local VehiclePacket
local VehicleGear
local VehicleRPM
local VehicleSpeed


local MainTab = Window:CreateTab("Speedtest", 10249261654)



local VehicleName = MainTab:CreateLabel("Vehicle Name: nil")
local Gears = MainTab:CreateLabel("Gear: 0")
local RPMs = MainTab:CreateLabel("RPM: 0")
local ActualSpeed = MainTab:CreateLabel("Top Speed: 0")

local ToggleSpeed = MainTab:CreateToggle({
	Name = "Toggle Speedtest",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function (Value)
		if Value then
			
			VehiclePacket = require(game:GetService('ReplicatedStorage').Game.Vehicle).GetLocalVehiclePacket()
			Vehicle = require(game:GetService('ReplicatedStorage').Game.Vehicle).GetLocalVehicleModel()
			enabled = true
			VehicleCFrame = Vehicle.PrimaryPart.CFrame
			VehicleName:Set("Vehicle Name: " .. Vehicle.Name)

Rayfield:Notify({
   Title = "Speedtest Enabled!",
   Content = "Have fun testing your Vehicle",
   Duration = 2.5,
   Image = 4643750660

})
			else

			
			VehicleGear = VehiclePacket.LastGear
			VehicleRPM = VehiclePacket.LastRPM

			VehiclePacket = nil
			Vehicle = nil
			enabled = false
			VehicleCFrame = nil
			
			Rayfield:Notify({
   Title = "Speedtest Disabled!",
   Content = "Speedtest is now disabled, you can drive again!",
   Duration = 2.5,
   Image = 4643750660

})
		end
		local rs = game:GetService("RunService")

		
		
		rs.Heartbeat:Connect(function()
		
		Gears:Set("Gear: " .. tostring(VehicleGear))
		RPMs:Set("RPM: " .. tostring(VehicleRPM))
		ActualSpeed:Set("Top Speed: " .. tostring(VehicleSpeed))
			if enabled then
				VehicleSpeed = game.Players.LocalPlayer.PlayerGui.AppUI.Speedometer.Top.Speed.Text
				VehicleGear = VehiclePacket.Gear
				VehicleRPM = VehiclePacket.rpmVisual
				Vehicle:SetPrimaryPartCFrame(VehicleCFrame)
			end
		end)
		
	end
})
